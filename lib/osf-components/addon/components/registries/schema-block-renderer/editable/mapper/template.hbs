{{yield (hash
    page-heading=(
        component
        'registries/schema-block-renderer/page-heading'
        isEditableForm=true
    )
    section-heading=(
        component
        'registries/schema-block-renderer/section-heading'
        isEditableForm=true
    )
    subsection-heading=(
        component
        'registries/schema-block-renderer/subsection-heading'
        isEditableForm=true
    )
    paragraph=(
        component
        'registries/schema-block-renderer/paragraph'
        isEditableForm=true
    )
    question-label=(
        component
        'registries/schema-block-renderer/label'
        isEditableForm=true
    )
    short-text-input=(
        component
        'registries/schema-block-renderer/editable/text'
        changeset=@changeset
        onInput=@onInput
    )
    long-text-input=(
        component
        'registries/schema-block-renderer/editable/textarea'
        changeset=@changeset
        onInput=@onInput
    )
    single-select-input=(
        component
        'registries/schema-block-renderer/editable/single-select-input'
        changeset=@changeset
        onInput=@onInput
    )
    multi-select-input=(
        component
        'registries/schema-block-renderer/editable/multi-select-input'
        changeset=@changeset
        onInput=@onInput
    )
    contributors-input=(
        component
        'registries/schema-block-renderer/read-only-contributor-list'
        node=@node
        isEditableForm=true
    )
    file-input=(
        component
        'registries/schema-block-renderer/editable/files'
        changeset=@changeset
        node=@node
        onInput=@onInput
    )
    e-rad-award-funder-input=(
        component
        'registries/schema-block-renderer/editable/rdm/e-rad-award-funder-input'
        changeset=@changeset
        onInput=@onInput
        node=@node
        metadataNodeErad=@metadataNodeErad
    )
    e-rad-award-number-input=(
        component
        'registries/schema-block-renderer/editable/rdm/e-rad-award-number-input'
        changeset=@changeset
        metadataChangeset=@metadataChangeset
        onInput=@onInput
        onMetadataInput=@onMetadataInput
        node=@node
        metadataNodeErad=@metadataNodeErad
    )
    e-rad-award-title-ja-input=(
        component
        'registries/schema-block-renderer/editable/rdm/e-rad-award-title-ja-input'
        changeset=@changeset
        metadataChangeset=@metadataChangeset
        onInput=@onInput
        onMetadataInput=@onMetadataInput
        node=@node
        metadataNodeErad=@metadataNodeErad
    )
    e-rad-award-title-en-input=(
        component
        'registries/schema-block-renderer/editable/rdm/e-rad-award-title-en-input'
        changeset=@changeset
        metadataChangeset=@metadataChangeset
        onInput=@onInput
        onMetadataInput=@onMetadataInput
        node=@node
        metadataNodeErad=@metadataNodeErad
    )
    file-metadata-input=(
        component
        'registries/schema-block-renderer/editable/rdm/file-metadata-input'
        changeset=@changeset
        onInput=@onInput
        node=@node
    )
)}}
