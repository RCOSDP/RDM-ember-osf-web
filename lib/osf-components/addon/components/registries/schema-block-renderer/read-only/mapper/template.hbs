{{yield (hash
    page-heading=(
        component
        'registries/schema-block-renderer/page-heading'
    )
    section-heading=(
        component
        'registries/schema-block-renderer/section-heading'
    )
    subsection-heading=(
        component
        'registries/schema-block-renderer/subsection-heading'
    )
    paragraph=(
        component
        'registries/schema-block-renderer/paragraph'
    )
    question-label=(
        component
        'registries/schema-block-renderer/label'
        readonly=true
        draftManager=@draftManager
    )
    short-text-input=(
        component
        'registries/schema-block-renderer/read-only/response'
        registrationResponses=@registrationResponses
        changeset=@changeset
    )
    long-text-input=(
        component
        'registries/schema-block-renderer/read-only/response'
        registrationResponses=@registrationResponses
        changeset=@changeset
    )
    single-select-input=(
        component
        'registries/schema-block-renderer/read-only/response'
        registrationResponses=@registrationResponses
        changeset=@changeset
    )
    multi-select-input=(
        component
        'registries/schema-block-renderer/read-only/multi-select'
        registrationResponses=@registrationResponses
        changeset=@changeset
    )
    contributors-input=(
        component
        'registries/schema-block-renderer/read-only-contributor-list'
        node=@node
    )
    file-input=(
        component
        'registries/schema-block-renderer/read-only/files'
        registrationResponses=@registrationResponses
        changeset=@changeset
    )
    e-rad-award-funder-input=(
        component
        'registries/schema-block-renderer/read-only/response'
        registrationResponses=@registrationResponses
        changeset=@changeset
    )
    e-rad-award-number-input=(
        component
        'registries/schema-block-renderer/read-only/response'
        registrationResponses=@registrationResponses
        changeset=@changeset
    )
    e-rad-award-title-ja-input=(
        component
        'registries/schema-block-renderer/read-only/response'
        registrationResponses=@registrationResponses
        changeset=@changeset
    )
    e-rad-award-title-en-input=(
        component
        'registries/schema-block-renderer/read-only/response'
        registrationResponses=@registrationResponses
        changeset=@changeset
    )
    file-metadata-input=(
        component
        'registries/schema-block-renderer/read-only/rdm/file-metadata-input'
        registrationResponses=@registrationResponses
        changeset=@changeset
        node=@node
    )
)}}
