{{#let (hash
    quickfiles=(component 'hyper-link'
        'guid-user.quickfiles'
        this.currentUser.currentUserId
        analyticsLabel='Navbar - navbar.my_quick_files'
        hidden=(or
            this.onInstitutions
            (not this.session.isAuthenticated)
            (not this.useNavQuickfiles)
        )
        tagName='li'
        text=(t 'navbar.my_quick_files')
        onClicked=this.onLinkClicked
    )
    myprojects=(component 'hyper-link'
        this.myProjectsURL
        analyticsLabel='Navbar - navbar.my_projects'
        hidden=(not this.session.isAuthenticated)
        tagName='li'
        text=(t 'navbar.my_projects')
        onClicked=this.onLinkClicked
    )
    myregistrations=(component 'hyper-link'
        this.myRegistrationsURL
        analyticsLabel='Navbar - navbar.my_registrations'
        hidden=(or
            (not this.session.isAuthenticated)
            (not this.useNavRegistrations)
        )
        tagName='li'
        text=(t 'navbar.my_registrations')
        onClicked=this.onLinkClicked
    )
    search=(component 'hyper-link'
        this.searchURL
        analyticsLabel='Navbar - navbar.search'
        hidden=(not this.useNavSearch)
        tagName='li'
        text=(t 'navbar.search')
        onClicked=this.onLinkClicked
    )
    support=(component 'hyper-link'
        this.supportURL
        analyticsLabel='Navbar - navbar.support'
        hidden=(not this.useNavSupport)
        tagName='li'
        text=(t 'navbar.support')
        onClicked=this.onLinkClicked
    )
    donate=(component 'hyper-link'
        'https://cos.io/donate'
        analyticsLabel='Navbar - navbar.donate'
        classNames='navbar-donate-button'
        hidden=(not this.useNavDonate)
        tagName='li'
        text=(t 'navbar.donate')
        onClicked=this.onLinkClicked
    )
) as |links|}}
    {{#if hasBlock}}
        {{yield links}}
    {{else}}
        {{links.quickfiles}}
        {{links.myprojects}}
        {{links.search}}
        {{links.support}}
        {{links.donate}}
    {{/if}}
{{/let}}
